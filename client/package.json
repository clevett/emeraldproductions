{
  "name": "emeraldproductions",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "fuse.js": "^6.2.1",
    "node": "^14.8.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.0",
    "react-ga": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-ga": "^1.2.3",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.4.1",
    "roll": "^1.3.0"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "start": "npm-run-all --parallel react:start sass:watch",
    "build": "npm-run-all --parallel react:build sass:compile",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "sass:watch": "node-sass src/scss/index.scss src/index.css -w",
    "sass:compile": "node-sass src/scss/index.scss src/index.css --style compressed",
    "node:server": "cd .. && node server.js",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/preset-env": "^7.10.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest-cli": "^26.6.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/mocks/fileMock.js"
    }
  }
}

{
  "name": "emeraldproductions",
  "version": "1.0.0",
  "description": "Emerald Productions tools",
  "main": "index.js",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "node:server": "node server.js",
    "server:start": "nodemon server.js",
    "start": "npm-run-all --parallel server:start react:start sass:watch",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build",
    "react:start": "react-scripts start",
    "build": "npm-run-all --parallel react:build sass:compile",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "sass:watch": "node-sass ./src/scss/index.scss ./src/index.css -w",
    "sass:compile": "node-sass ./src/scss/index.scss ./src/index.css",
    "test": "jest --watch"
  },
  "author": "Cassie Levett",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "fuse.js": "^6.2.1",
    "node": "^14.8.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.4.1",
    "roll": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.2",
    "nodemailer": "^6.4.8",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2"
  }
}
